<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.lzhiy.anyandroiddemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.RESTART_PACKAGES"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".about_layout.AboutLayoutActivity">
        </activity>
        <activity android:name=".about_layout.about_keyboard.AboutKeyboardActivity">
        </activity>
        <activity
            android:name=".about_layout.about_keyboard.LayoutUpActivity"
            android:windowSoftInputMode="adjustPan">
        </activity>
        <activity android:name=".performance.PerformanceActivity">
        </activity>
        <activity android:name=".performance.LayoutOptimizeActivity">
        </activity>

        <!-- <receiver android:name=".receiver.InstallAndUninstallActivity"> -->
